      "name": "22 - Usuarios - Historial (user1)",
      "request": { "method": "GET", "header": [], "url": { "raw": "{{baseUrl}}/api/usuarios/{{user1Id}}/historial", "host": ["{{baseUrl}}"], "path": ["api","usuarios","{{user1Id}}","historial"] } },
